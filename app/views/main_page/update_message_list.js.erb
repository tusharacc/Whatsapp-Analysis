$('#media-added').text('<%= @media_added %>');
$('#members-added').text('<%= @members_added %>');
$('#whatsappdata').css('display','inline');
$('#message-by-each').css('display','inline');
$('#rendering_partial').html("<%= j render(:partial => 'main_page/summary') %>");

Morris.Bar({
  element: 'message-by-each',
  data: $('#message-by-each').data('messages'),
  xkey: 'name',
  ykeys: ['count'],
  labels: ['Message Count by each member']
});

Morris.Bar({
  element: 'message-by-day',
  data: $('#message-by-day').data('messages-per-day'),
  xkey: 'day',
  ykeys: ['count'],
  labels: ['Message Count per day']
});

Morris.Bar({
  element: 'message-by-time-slot',
  data: $('#message-by-time-slot').data('messages-per-time-slot'),
  xkey: 'slot',
  ykeys: ['count'],
  labels: ['Message Count per time slot']
});

Morris.Bar({
  element: 'message-by-month',
  data: $('#message-by-month').data('messages-per-month'),
  xkey: 'month',
  ykeys: ['count'],
  labels: ['Message Count per month']
});